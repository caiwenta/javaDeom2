<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="table.ApiQueueEntity" table="api_queue" schema="dbo" catalog="wdd_erp_snsn">
        <id name="tdocId" column="tdoc_id">
            <!--id中参数的设置问:native/assigned/foreign/increment-->
            <generator class="native"></generator>
        </id>
        <property name="tdocMethod" column="tdoc_method"/>
        <property name="tdocXml" column="tdoc_xml"/>
        <property name="tdocRemark" column="tdoc_remark"/>
        <property name="tdocState" column="tdoc_state"/>
        <property name="tdocAddtime" column="tdoc_addtime"/>
        <property name="tdocUpdates" column="tdoc_updates"/>
        <property name="tdocUpdatime" column="tdoc_updatime"/>
        <property name="tdocHash" column="tdoc_hash"/>
        <property name="wddtaskcode" column="wddtaskcode"/>
        <property name="tdocErpId" column="tdoc_erp_id"/>
        <property name="tdocApitoken" column="tdoc_apitoken"/>
        <property name="tdocCpId" column="tdoc_cp_id"/>
        <property name="tdocLevel" column="tdoc_level"/>
        <property name="tdocDequeuedate" column="tdoc_dequeuedate"/>
        <property name="tdocTarget" column="tdoc_target"/>
        <property name="tdocAction" column="tdoc_action"/>
        <property name="tdocShid" column="tdoc_shid"/>
        <property name="tdocNo" column="tdoc_no"/>
        <property name="tdocPriority" column="tdoc_priority"/>
        <property name="tdocElapsed" column="tdoc_elapsed"/>
        <property name="tdocTotal" column="tdoc_total"/>
        <property name="tdocBeatdate" column="tdoc_beatdate"/>
    </class>
</hibernate-mapping>